</span><span><span class="hljs-comment">// Add a new shipment</span></span><span>
app.</span><span><span class="hljs-title function_">post</span></span><span>(</span><span><span class="hljs-string">'/add-load'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { loadNumber, origin, destination, status } = req.</span><span><span class="hljs-property">body</span></span><span>;

  </span><span><span class="hljs-comment">// Insert a new shipment into the database</span></span><span>
  </span><span><span class="hljs-keyword">const</span></span><span> query = </span><span><span class="hljs-string">`INSERT INTO shipments (loadNumber, origin, destination, status) VALUES (?, ?, ?, ?)`</span></span><span>;
  db.</span><span><span class="hljs-title function_">run</span></span><span>(query, [loadNumber, origin, destination, status], </span><span><span class="hljs-keyword">function</span></span><span> (</span><span><span class="hljs-params">err</span></span><span>) {
    </span><span><span class="hljs-keyword">if</span></span><span> (err) {
      </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(err.</span><span><span class="hljs-property">message</span></span><span>);
      </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Error saving load'</span></span><span>);
    }
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">201</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">id</span></span><span>: </span><span><span class="hljs-variable language_">this</span></span><span>.</span><span><span class="hljs-property">lastID</span></span><span> });
  });
});

</span><span><span class="hljs-comment">// Serve the index.html file</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  res.</span><span><span class="hljs-title function_">sendFile</span></span><span>(path.</span><span><span class="hljs-title function_">join</span></span><span>(__dirname, </span><span><span class="hljs-string">'public'</span></span><span>, </span><span><span class="hljs-string">'index.html'</span></span><span>));
});

</span><span><span class="hljs-comment">// Start the server</span></span><span>
app.</span><span><span class="hljs-title function_">listen</span></span><span>(port, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
  </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">`Server running at http://localhost:<span class="hljs-subst">${port}</span></span></span><span>`);
});
