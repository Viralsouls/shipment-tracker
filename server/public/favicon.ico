    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">label</span></span></span><span>&gt;Destination: </span><span><span class="hljs-tag">&lt;<span class="hljs-name">input</span></span></span><span> </span><span><span class="hljs-attr">type</span></span><span>=</span><span><span class="hljs-string">"text"</span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"destination"</span></span><span> </span><span><span class="hljs-attr">required</span></span><span> /&gt;</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">label</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">label</span></span></span><span>&gt;Carrier: </span><span><span class="hljs-tag">&lt;<span class="hljs-name">input</span></span></span><span> </span><span><span class="hljs-attr">type</span></span><span>=</span><span><span class="hljs-string">"text"</span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"carrier"</span></span><span> /&gt;</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">label</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">label</span></span></span><span>&gt;Status:
      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">select</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"status"</span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"In Transit"</span></span><span>&gt;In Transit</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"Delivered"</span></span><span>&gt;Delivered</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">option</span></span></span><span> </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">"Delayed"</span></span><span>&gt;Delayed</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">option</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">select</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">label</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">label</span></span></span><span>&gt;Notes: </span><span><span class="hljs-tag">&lt;<span class="hljs-name">input</span></span></span><span> </span><span><span class="hljs-attr">type</span></span><span>=</span><span><span class="hljs-string">"text"</span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"notes"</span></span><span> /&gt;</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">label</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">button</span></span></span><span> </span><span><span class="hljs-attr">type</span></span><span>=</span><span><span class="hljs-string">"submit"</span></span><span>&gt;Add Shipment</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">button</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">form</span></span></span><span>&gt;

  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">h2</span></span></span><span>&gt;Shipments</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">h2</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">table</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"shipmentTable"</span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">thead</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">tr</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Origin</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;</span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Destination</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;</span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Carrier</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;</span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Status</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;</span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Notes</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">tr</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">thead</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">tbody</span></span></span><span>&gt;</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">tbody</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">table</span></span></span><span>&gt;

  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">script</span></span></span><span>&gt;<span class="language-javascript">
    </span></span><span><span class="hljs-keyword">const</span></span><span> form = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'shipmentForm'</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> tableBody = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">querySelector</span></span><span>(</span><span><span class="hljs-string">'#shipmentTable tbody'</span></span><span>);

    form.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">'submit'</span></span><span>, </span><span><span class="hljs-keyword">async</span></span><span> (e) =&gt; {
      e.</span><span><span class="hljs-title function_">preventDefault</span></span><span>();

      </span><span><span class="hljs-keyword">const</span></span><span> shipment = {
        </span><span><span class="hljs-attr">origin</span></span><span>: </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'origin'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>,
        </span><span><span class="hljs-attr">destination</span></span><span>: </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'destination'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>,
        </span><span><span class="hljs-attr">carrier</span></span><span>: </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'carrier'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>,
        </span><span><span class="hljs-attr">status</span></span><span>: </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'status'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>,
        </span><span><span class="hljs-attr">notes</span></span><span>: </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'notes'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>
      };

      </span><span><span class="hljs-keyword">const</span></span><span> response = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">fetch</span></span><span>(</span><span><span class="hljs-string">'http://localhost:3000/shipments'</span></span><span>, {
        </span><span><span class="hljs-attr">method</span></span><span>: </span><span><span class="hljs-string">'POST'</span></span><span>,
        </span><span><span class="hljs-attr">headers</span></span><span>: { </span><span><span class="hljs-string">'Content-Type'</span></span><span>: </span><span><span class="hljs-string">'application/json'</span></span><span> },
        </span><span><span class="hljs-attr">body</span></span><span>: </span><span><span class="hljs-title class_">JSON</span></span><span>.</span><span><span class="hljs-title function_">stringify</span></span><span>(shipment)
      });

      </span><span><span class="hljs-keyword">const</span></span><span> result = </span><span><span class="hljs-keyword">await</span></span><span> response.</span><span><span class="hljs-title function_">json</span></span><span>();
      </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">'Saved:'</span></span><span>, result);
      </span><span><span class="hljs-title function_">addRowToTable</span></span><span>(result);
      form.</span><span><span class="hljs-title function_">reset</span></span><span>();
    });

    </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">addRowToTable</span></span><span>(</span><span><span class="hljs-params">data</span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> row = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">'tr'</span></span><span>);
      row.</span><span><span class="hljs-property">innerHTML</span></span><span> = <span class="hljs-string">`
        &lt;td&gt;</span></span><span><span class="hljs-subst">${data.origin}</span></span><span>&lt;/td&gt;
        &lt;td&gt;</span><span><span class="hljs-subst">${data.destination}</span></span><span>&lt;/td&gt;
        &lt;td&gt;</span><span><span class="hljs-subst">${data.carrier}</span></span><span>&lt;/td&gt;
        &lt;td&gt;</span><span><span class="hljs-subst">${data.status}</span></span><span>&lt;/td&gt;
        &lt;td&gt;</span><span><span class="hljs-subst">${data.notes}</span></span><span>&lt;/td&gt;
      `;
      tableBody.</span><span><span class="hljs-title function_">appendChild</span></span><span>(row);
    }
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">script</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">body</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">html</span></span></span><span>&gt;
</span></span></code></div></div></pre>
