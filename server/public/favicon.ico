      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">input</span></span></span><span> </span><span><span class="hljs-attr">type</span></span><span>=</span><span><span class="hljs-string">"text"</span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"status"</span></span><span> </span><span><span class="hljs-attr">name</span></span><span>=</span><span><span class="hljs-string">"status"</span></span><span> </span><span><span class="hljs-attr">required</span></span><span>&gt;</span><span><span class="hljs-tag">&lt;<span class="hljs-name">br</span></span></span><span>&gt;

