</span><span><span class="hljs-comment">// Get all shipments</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/api/shipments'</span></span><span>, </span><span><span class="hljs-keyword">async</span></span><span> (req, res) =&gt; {
