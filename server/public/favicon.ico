  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">meta</span></span></span><span> </span><span><span class="hljs-attr">charset</span></span><span>=</span><span><span class="hljs-string">"UTF-8"</span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">meta</span></span></span><span> </span><span><span class="hljs-attr">name</span></span><span>=</span><span><span class="hljs-string">"viewport"</span></span><span> </span><span><span class="hljs-attr">content</span></span><span>=</span><span><span class="hljs-string">"width=device-width, initial-scale=1.0"</span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">title</span></span></span><span>&gt;Shipment Tracker</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">title</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">head</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;<span class="hljs-name">body</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">h1</span></span></span><span>&gt;Shipment Tracker</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">h1</span></span></span><span>&gt;

  </span><span><span class="hljs-comment">&lt;!-- Search Bar --&gt;</span></span><span>
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">input</span></span></span><span> </span><span><span class="hljs-attr">type</span></span><span>=</span><span><span class="hljs-string">"text"</span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"search-bar"</span></span><span> </span><span><span class="hljs-attr">placeholder</span></span><span>=</span><span><span class="hljs-string">"Search by Load Number, Origin, or Destination"</span></span><span> </span><span><span class="hljs-attr">onkeyup</span></span><span>=</span><span><span class="hljs-string">"searchShipments()"</span></span><span>&gt;

  </span><span><span class="hljs-comment">&lt;!-- Table to show shipments --&gt;</span></span><span>
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">table</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">thead</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;<span class="hljs-name">tr</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Load Number</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Origin</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Destination</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;
        </span><span><span class="hljs-tag">&lt;<span class="hljs-name">th</span></span></span><span>&gt;Status</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">th</span></span></span><span>&gt;
      </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">tr</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">thead</span></span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">tbody</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"table-body"</span></span><span>&gt;
      </span><span><span class="hljs-comment">&lt;!-- Table rows will be added here dynamically --&gt;</span></span><span>
    </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">tbody</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">table</span></span></span><span>&gt;

  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">script</span></span></span><span>&gt;<span class="language-javascript">
    </span></span><span><span class="hljs-keyword">const</span></span><span> tableBody = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'table-body'</span></span><span>);

    </span><span><span class="hljs-comment">// Function to fetch shipments and display them</span></span><span>
    </span><span><span class="hljs-keyword">async</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">loadShipments</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> response = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">fetch</span></span><span>(</span><span><span class="hljs-string">'http://localhost:3000/shipments'</span></span><span>);
      </span><span><span class="hljs-keyword">const</span></span><span> shipments = </span><span><span class="hljs-keyword">await</span></span><span> response.</span><span><span class="hljs-title function_">json</span></span><span>();
      tableBody.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">''</span></span><span>;
      shipments.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">shipment, index</span></span></span><span>) =&gt; </span><span><span class="hljs-title function_">addRowToTable</span></span><span>(shipment, index));
    }

    </span><span><span class="hljs-comment">// Function to add a row in the table</span></span><span>
    </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">addRowToTable</span></span><span>(</span><span><span class="hljs-params">shipment, index</span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> row = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">'tr'</span></span><span>);
      row.</span><span><span class="hljs-property">innerHTML</span></span><span> = <span class="hljs-string">`
        &lt;td&gt;</span></span><span><span class="hljs-subst">${shipment.loadNumber}</span></span><span>&lt;/td&gt;
        &lt;td&gt;</span><span><span class="hljs-subst">${shipment.origin}</span></span><span>&lt;/td&gt;
        &lt;td&gt;</span><span><span class="hljs-subst">${shipment.destination}</span></span><span>&lt;/td&gt;
        &lt;td&gt;</span><span><span class="hljs-subst">${shipment.status}</span></span><span>&lt;/td&gt;
      `;
      tableBody.</span><span><span class="hljs-title function_">appendChild</span></span><span>(row);
    }

    </span><span><span class="hljs-comment">// Function to search shipments by Load Number, Origin, or Destination</span></span><span>
    </span><span><span class="hljs-keyword">async</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">searchShipments</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> searchQuery = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'search-bar'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>.</span><span><span class="hljs-title function_">trim</span></span><span>().</span><span><span class="hljs-title function_">toLowerCase</span></span><span>();
      </span><span><span class="hljs-keyword">const</span></span><span> response = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">fetch</span></span><span>(</span><span><span class="hljs-string">'http://localhost:3000/shipments'</span></span><span>);
      </span><span><span class="hljs-keyword">const</span></span><span> shipments = </span><span><span class="hljs-keyword">await</span></span><span> response.</span><span><span class="hljs-title function_">json</span></span><span>();

      </span><span><span class="hljs-comment">// Filter by Load Number, Origin, or Destination</span></span><span>
      </span><span><span class="hljs-keyword">const</span></span><span> filteredShipments = shipments.</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">shipment</span></span></span><span> =&gt; 
        shipment.</span><span><span class="hljs-property">loadNumber</span></span><span>.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>().</span><span><span class="hljs-title function_">includes</span></span><span>(searchQuery) ||
        shipment.</span><span><span class="hljs-property">origin</span></span><span>.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>().</span><span><span class="hljs-title function_">includes</span></span><span>(searchQuery) ||
        shipment.</span><span><span class="hljs-property">destination</span></span><span>.</span><span><span class="hljs-title function_">toLowerCase</span></span><span>().</span><span><span class="hljs-title function_">includes</span></span><span>(searchQuery)
      );

      tableBody.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">''</span></span><span>;
      filteredShipments.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">shipment, index</span></span></span><span>) =&gt; </span><span><span class="hljs-title function_">addRowToTable</span></span><span>(shipment, index));
    }

    </span><span><span class="hljs-comment">// Load all shipments when the page is loaded</span></span><span>
    </span><span><span class="hljs-variable language_">window</span></span><span>.</span><span><span class="hljs-property">onload</span></span><span> = loadShipments;
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">script</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">body</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">html</span></span></span><span>&gt;
</span></span></code></div></div></pre>
