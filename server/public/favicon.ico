    db.</span><span><span class="hljs-title function_">run</span></span><span>(query, [load_number, origin, destination, status], </span><span><span class="hljs-keyword">function</span></span><span> (</span><span><span class="hljs-params">err</span></span><span>) {
        </span><span><span class="hljs-keyword">if</span></span><span> (err) {
