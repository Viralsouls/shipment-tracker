<pre class="overflow-visible!" data-start="481" data-end="2876"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none rounded-t-[5px]">javascript</div><div class="sticky top-9"><div class="absolute end-0 bottom-0 flex h-9 items-center pe-2"><div class="bg-token-sidebar-surface-primary text-token-text-secondary dark:bg-token-main-surface-secondary flex items-center rounded-sm px-2 font-sans text-xs"><button class="flex gap-1 items-center select-none px-4 py-1" aria-label="Copy"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-xs"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copy</button><span class="" data-state="closed"><button class="flex items-center gap-1 px-4 py-1 select-none"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-xs"><path d="M2.5 5.5C4.3 5.2 5.2 4 5.5 2.5C5.8 4 6.7 5.2 8.5 5.5C6.7 5.8 5.8 7 5.5 8.5C5.2 7 4.3 5.8 2.5 5.5Z" fill="currentColor" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"></path><path d="M5.66282 16.5231L5.18413 19.3952C5.12203 19.7678 5.09098 19.9541 5.14876 20.0888C5.19933 20.2067 5.29328 20.3007 5.41118 20.3512C5.54589 20.409 5.73218 20.378 6.10476 20.3159L8.97693 19.8372C9.72813 19.712 10.1037 19.6494 10.4542 19.521C10.7652 19.407 11.0608 19.2549 11.3343 19.068C11.6425 18.8575 11.9118 18.5882 12.4503 18.0497L20 10.5C21.3807 9.11929 21.3807 6.88071 20 5.5C18.6193 4.11929 16.3807 4.11929 15 5.5L7.45026 13.0497C6.91175 13.5882 6.6425 13.8575 6.43197 14.1657C6.24513 14.4392 6.09299 14.7348 5.97903 15.0458C5.85062 15.3963 5.78802 15.7719 5.66282 16.5231Z" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path><path d="M14.5 7L18.5 11" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"></path></svg>Edit</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-javascript"><span><span><span class="hljs-keyword">const</span></span><span> express = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">'express'</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> path = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">'path'</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> app = </span><span><span class="hljs-title function_">express</span></span><span>();
</span><span><span class="hljs-keyword">const</span></span><span> sqlite3 = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">'better-sqlite3'</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> db = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title function_">sqlite3</span></span><span>(</span><span><span class="hljs-string">'./server/shipment_tracker.db'</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> port = </span><span><span class="hljs-number">3000</span></span><span>;

</span><span><span class="hljs-comment">// Middleware for parsing JSON requests</span></span><span>
app.</span><span><span class="hljs-title function_">use</span></span><span>(express.</span><span><span class="hljs-title function_">json</span></span><span>());
app.</span><span><span class="hljs-title function_">use</span></span><span>(express.</span><span><span class="hljs-title function_">static</span></span><span>(path.</span><span><span class="hljs-title function_">join</span></span><span>(__dirname, </span><span><span class="hljs-string">'public'</span></span><span>)));

</span><span><span class="hljs-comment">// API to fetch all shipments</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/api/shipments'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> shipments = db.</span><span><span class="hljs-title function_">prepare</span></span><span>(</span><span><span class="hljs-string">'SELECT * FROM shipments'</span></span><span>).</span><span><span class="hljs-title function_">all</span></span><span>();
    res.</span><span><span class="hljs-title function_">json</span></span><span>(shipments);
  } </span><span><span class="hljs-keyword">catch</span></span><span> (err) {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(err);
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Error fetching shipments'</span></span><span>);
  }
});

</span><span><span class="hljs-comment">// API to add a new load</span></span><span>
app.</span><span><span class="hljs-title function_">post</span></span><span>(</span><span><span class="hljs-string">'/add-load'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { origin, destination, status } = req.</span><span><span class="hljs-property">body</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> load_id = </span><span><span class="hljs-string">'LD'</span></span><span> + </span><span><span class="hljs-title class_">Date</span></span><span>.</span><span><span class="hljs-title function_">now</span></span><span>().</span><span><span class="hljs-title function_">toString</span></span><span>().</span><span><span class="hljs-title function_">slice</span></span><span>(-</span><span><span class="hljs-number">10</span></span><span>); </span><span><span class="hljs-comment">// Generate unique load ID</span></span><span>

  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> stmt = db.</span><span><span class="hljs-title function_">prepare</span></span><span>(</span><span><span class="hljs-string">'INSERT INTO shipments (load_id, origin, destination, status) VALUES (?, ?, ?, ?)'</span></span><span>);
    stmt.</span><span><span class="hljs-title function_">run</span></span><span>(load_id, origin, destination, status);
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">201</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Load added successfully'</span></span><span>);
  } </span><span><span class="hljs-keyword">catch</span></span><span> (err) {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(err);
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Error adding load'</span></span><span>);
  }
});

</span><span><span class="hljs-comment">// API to edit a load</span></span><span>
app.</span><span><span class="hljs-title function_">put</span></span><span>(</span><span><span class="hljs-string">'/edit-load/:id'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { id } = req.</span><span><span class="hljs-property">params</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> { origin, destination, status } = req.</span><span><span class="hljs-property">body</span></span><span>;

  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> stmt = db.</span><span><span class="hljs-title function_">prepare</span></span><span>(</span><span><span class="hljs-string">'UPDATE shipments SET origin = ?, destination = ?, status = ? WHERE id = ?'</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> result = stmt.</span><span><span class="hljs-title function_">run</span></span><span>(origin, destination, status, id);

    </span><span><span class="hljs-keyword">if</span></span><span> (result.</span><span><span class="hljs-property">changes</span></span><span> &gt; </span><span><span class="hljs-number">0</span></span><span>) {
      res.</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Load updated successfully'</span></span><span>);
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">404</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Load not found'</span></span><span>);
    }
  } </span><span><span class="hljs-keyword">catch</span></span><span> (err) {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(err);
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Error updating load'</span></span><span>);
  }
});

</span><span><span class="hljs-comment">// API to delete a load</span></span><span>
app.</span><span><span class="hljs-title function_">delete</span></span><span>(</span><span><span class="hljs-string">'/delete-load/:id'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { id } = req.</span><span><span class="hljs-property">params</span></span><span>;

  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> stmt = db.</span><span><span class="hljs-title function_">prepare</span></span><span>(</span><span><span class="hljs-string">'DELETE FROM shipments WHERE id = ?'</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> result = stmt.</span><span><span class="hljs-title function_">run</span></span><span>(id);

    </span><span><span class="hljs-keyword">if</span></span><span> (result.</span><span><span class="hljs-property">changes</span></span><span> &gt; </span><span><span class="hljs-number">0</span></span><span>) {
      res.</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Load deleted successfully'</span></span><span>);
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">404</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Load not found'</span></span><span>);
    }
  } </span><span><span class="hljs-keyword">catch</span></span><span> (err) {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(err);
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">send</span></span><span>(</span><span><span class="hljs-string">'Error deleting load'</span></span><span>);
  }
});

</span><span><span class="hljs-comment">// Serve index.html file</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  res.</span><span><span class="hljs-title function_">sendFile</span></span><span>(path.</span><span><span class="hljs-title function_">join</span></span><span>(__dirname, </span><span><span class="hljs-string">'../public/index.html'</span></span><span>));
});

</span><span><span class="hljs-comment">// Start the server</span></span><span>
app.</span><span><span class="hljs-title function_">listen</span></span><span>(port, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
  </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">`Server running at http://localhost:<span class="hljs-subst">${port}</span></span></span><span>`);
});
</span></span></code></div></div></pre>
