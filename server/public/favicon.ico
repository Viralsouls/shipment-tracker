</span><span><span class="hljs-keyword">const</span></span><span> app = </span><span><span class="hljs-title function_">express</span></span><span>();
</span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-variable constant_">PORT</span></span><span> = </span><span><span class="hljs-number">3000</span></span><span>;

app.</span><span><span class="hljs-title function_">use</span></span><span>(express.</span><span><span class="hljs-title function_">json</span></span><span>());

</span><span><span class="hljs-comment">// Serve static files from the public folder</span></span><span>
app.</span><span><span class="hljs-title function_">use</span></span><span>(express.</span><span><span class="hljs-title function_">static</span></span><span>(path.</span><span><span class="hljs-title function_">join</span></span><span>(__dirname, </span><span><span class="hljs-string">'../public'</span></span><span>)));

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">generateLoadId</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
  </span><span><span class="hljs-keyword">const</span></span><span> random = </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">floor</span></span><span>(</span><span><span class="hljs-number">1000</span></span><span> + </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">random</span></span><span>() * </span><span><span class="hljs-number">9000</span></span><span>);
  </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">`LD<span class="hljs-subst">${random}</span></span></span><span>`;
}

</span><span><span class="hljs-comment">// API to get all shipments</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/api/shipments'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> rows = db.</span><span><span class="hljs-title function_">prepare</span></span><span>(</span><span><span class="hljs-string">'SELECT * FROM shipments'</span></span><span>).</span><span><span class="hljs-title function_">all</span></span><span>();
  res.</span><span><span class="hljs-title function_">json</span></span><span>(rows);
});

</span><span><span class="hljs-comment">// API to create new shipment</span></span><span>
app.</span><span><span class="hljs-title function_">post</span></span><span>(</span><span><span class="hljs-string">'/api/shipments'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { origin, destination, status } = req.</span><span><span class="hljs-property">body</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> loadId = </span><span><span class="hljs-title function_">generateLoadId</span></span><span>();
  db.</span><span><span class="hljs-title function_">prepare</span></span><span>(</span><span><span class="hljs-string">'INSERT INTO shipments (loadId, origin, destination, status) VALUES (?, ?, ?, ?)'</span></span><span>)
    .</span><span><span class="hljs-title function_">run</span></span><span>(loadId, origin, destination, status);
  res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">201</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-string">'Shipment added'</span></span><span>, loadId });
});

</span><span><span class="hljs-comment">// Catch-all to serve index.html for any GET request (SPA-safe)</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'*'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  res.</span><span><span class="hljs-title function_">sendFile</span></span><span>(path.</span><span><span class="hljs-title function_">join</span></span><span>(__dirname, </span><span><span class="hljs-string">'../public/index.html'</span></span><span>));
});

app.</span><span><span class="hljs-title function_">listen</span></span><span>(</span><span><span class="hljs-variable constant_">PORT</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
  </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">`🚚 Server is running on http://localhost:<span class="hljs-subst">${PORT}</span></span></span><span>`);
});
