  </span><span><span class="hljs-keyword">const</span></span><span> timestamp = </span><span><span class="hljs-title class_">Date</span></span><span>.</span><span><span class="hljs-title function_">now</span></span><span>();
  </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-string">`LD<span class="hljs-subst">${timestamp}</span></span></span><span>`;
};

</span><span><span class="hljs-comment">// Route to add a new load</span></span><span>
app.</span><span><span class="hljs-title function_">post</span></span><span>(</span><span><span class="hljs-string">'/add-load'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { origin, destination, status } = req.</span><span><span class="hljs-property">body</span></span><span>;

  </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">'Received request to add load:'</span></span><span>, req.</span><span><span class="hljs-property">body</span></span><span>);  </span><span><span class="hljs-comment">// Debugging log</span></span><span>

  </span><span><span class="hljs-keyword">if</span></span><span> (!origin || !destination || !status) {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">'Missing required fields'</span></span><span>);  </span><span><span class="hljs-comment">// Debugging log</span></span><span>
    </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">400</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">error</span></span><span>: </span><span><span class="hljs-string">'Missing required fields'</span></span><span> });
  }

  </span><span><span class="hljs-keyword">const</span></span><span> loadId = </span><span><span class="hljs-title function_">generateLoadId</span></span><span>(); </span><span><span class="hljs-comment">// Generate unique load ID</span></span><span>
  
  </span><span><span class="hljs-keyword">const</span></span><span> sql = </span><span><span class="hljs-string">`INSERT INTO shipments (load_id, origin, destination, status) VALUES (?, ?, ?, ?)`</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> params = [loadId, origin, destination, status];

  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">'Executing SQL query:'</span></span><span>, sql, params);  </span><span><span class="hljs-comment">// Debugging log</span></span><span>
    db.</span><span><span class="hljs-title function_">prepare</span></span><span>(sql).</span><span><span class="hljs-title function_">run</span></span><span>(...params);  </span><span><span class="hljs-comment">// Insert the data into the database</span></span><span>
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">`Load added successfully with ID: <span class="hljs-subst">${loadId}</span></span></span><span>`);  </span><span><span class="hljs-comment">// Debugging log</span></span><span>
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">200</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-string">'Load added successfully'</span></span><span>, </span><span><span class="hljs-attr">loadId</span></span><span>: loadId });
  } </span><span><span class="hljs-keyword">catch</span></span><span> (err) {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(</span><span><span class="hljs-string">'Error adding load:'</span></span><span>, err.</span><span><span class="hljs-property">message</span></span><span>);  </span><span><span class="hljs-comment">// Debugging log</span></span><span>
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">error</span></span><span>: </span><span><span class="hljs-string">'Error adding load'</span></span><span> });
  }
});

</span><span><span class="hljs-comment">// Route to get all loads</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/api/shipments'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> sql = </span><span><span class="hljs-string">`SELECT * FROM shipments`</span></span><span>;
  
  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">'Fetching all shipments'</span></span><span>);  </span><span><span class="hljs-comment">// Debugging log</span></span><span>
    </span><span><span class="hljs-keyword">const</span></span><span> rows = db.</span><span><span class="hljs-title function_">prepare</span></span><span>(sql).</span><span><span class="hljs-title function_">all</span></span><span>();  </span><span><span class="hljs-comment">// Fetch all rows from the database</span></span><span>
    res.</span><span><span class="hljs-title function_">json</span></span><span>(rows);
  } </span><span><span class="hljs-keyword">catch</span></span><span> (err) {
    </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(</span><span><span class="hljs-string">'Error fetching shipments:'</span></span><span>, err.</span><span><span class="hljs-property">message</span></span><span>);  </span><span><span class="hljs-comment">// Debugging log</span></span><span>
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">error</span></span><span>: err.</span><span><span class="hljs-property">message</span></span><span> });
  }
});

</span><span><span class="hljs-comment">// Serve the static frontend (index.html)</span></span><span>
app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/'</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">req, res</span></span></span><span>) =&gt; {
  res.</span><span><span class="hljs-title function_">sendFile</span></span><span>(path.</span><span><span class="hljs-title function_">join</span></span><span>(__dirname, </span><span><span class="hljs-string">'../public/index.html'</span></span><span>));
});

app.</span><span><span class="hljs-title function_">listen</span></span><span>(port, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
  </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">`Server running at http://localhost:<span class="hljs-subst">${port}</span></span></span><span>`);
});
</span></span></code></div></div></pre>
